<%- include('./layouts/header'); -%>

<% if (alert) { %>
    <h2><%= alert %></h2>
<% } %>
<form method="post" action="/updateEspecialidade/<%= result.id %>">
    <div class="mb-3">
      <label for="nomeEspecialidade" class="form-label">Nome da Especilidade</label>
      <input type="text" class="form-control" id="nomeEspecialidade" name="nomeEspecialidade" value= <%= result.nome %>>
    </div>
    <button type="submit" class="btn btn-primary">Salvar</button>
</form>
<%- include('./layouts/footer'); -%>